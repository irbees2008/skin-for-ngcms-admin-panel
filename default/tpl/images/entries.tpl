<tr>
	<td><input type="checkbox" name="files[]" value="{id}" /></td>
	<td>{id}</td>
	<td><span class="img-title">{file_name}</span></td>
	<td nowrap>[preview]{preview_img} {preview_size}[/preview] <span class="img-src">{view_file}</span> {view_thumb}</td>
	<td nowrap>{insert_file} {insert_thumb} {insert_preview}</td>
	<td>{folder}</td>
	<td>{user}</td>
	<td nowrap><span class="img-width">{width}</span>x<span class="img-height">{height}</span> <span class="img-size">{size}</span></td>
	<td nowrap>{rename} <a href="{edit_link}">{l_edit}</a></td>
</tr>