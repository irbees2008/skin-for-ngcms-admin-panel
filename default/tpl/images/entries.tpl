<tr>
	<td><input type="checkbox" name="files[]" value="{id}" /></td>
	<td>{id}</td>
	<td>{file_name}</td>
	[preview]<td>{preview_img} {preview_size}</td>[/preview]
	<td nowrap>{view_file} {view_thumb}</td>
	<td nowrap>{insert_file} {insert_thumb} {insert_preview}</td>
	<td nowrap>{rename} <a href="{edit_link}">{l_edit}</a></td>
	<td>{folder}</td>
	<td>{user}</td>
	<td nowrap>{width}x{height} {size}</td>
</tr>